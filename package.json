{
  "name": "algorithms-with-predictions",
  "version": "1.0.0",
  "private": true,
  "description": "algorithms-with-predictions",
  "author": "Alexander Lindermayr",
  "keywords": [
    "gatsby",
    "algorithms",
    "machine-learning",
    "predictions",
    "research",
    "academic"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "develop": "node scripts/composeData.js && gatsby develop",
    "start": "node scripts/composeData.js && gatsby develop",
    "build": "node scripts/composeData.js && gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"scripts/**/*.{js,mjs}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"scripts/**/*.{js,mjs}\" \"*.{js,json,md}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "gatsby clean",
    "clean:all": "gatsby clean && rm -rf node_modules package-lock.json",
    "deploy": "node scripts/composeData.js && gatsby build && gh-pages -d public -b deploy",
    "analyze": "npm run build && npx gatsby-plugin-webpack-bundle-analyzer-v2",
    "update-data": "node scripts/updateData.mjs",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/material": "^5.18.0",
    "@mui/icons-material": "^5.18.0",
    "gatsby": "^5.15.0",
    "gatsby-plugin-emotion": "^8.15.0",
    "gatsby-plugin-google-gtag": "^5.15.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "^7.15.0",
    "gatsby-remark-prismjs-copy-button": "^0.4.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "axios": "^1.13.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fast-levenshtein": "^3.0.0",
    "fast-xml-parser": "^4.5.3",
    "gatsby-remark-code-headers": "^1.1.0",
    "gh-pages": "^6.3.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  }
}