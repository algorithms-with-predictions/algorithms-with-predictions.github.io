{
  "name": "algorithms-with-predictions",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "algorithms-with-predictions",
  "author": "Alexander Lindermayr",
  "keywords": [
    "vite",
    "react",
    "algorithms",
    "machine-learning",
    "predictions",
    "research",
    "academic"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node scripts/composeData.js && vite",
    "start": "npm run dev",
    "build": "node scripts/composeData.js && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"scripts/**/*.{js,mjs}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"scripts/**/*.{js,mjs}\" \"*.{js,json,md}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "deploy": "npm run build && gh-pages -d dist -b deploy",
    "update-data": "node scripts/updateData.mjs",
    "prepare": "husky install",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "gtag": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.5",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.15",
    "axios": "^1.13.1",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "fast-levenshtein": "^3.0.0",
    "fast-xml-parser": "^4.5.3",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.6",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "vite": "^6.4.1",
    "vitest": "^4.0.15"
  }
}